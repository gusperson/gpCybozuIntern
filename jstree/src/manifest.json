{
  "manifest_version": 1,
  "version": 1,
  "type": "APP",
  "desktop": {
    "js": [
      "https://js.kintone.com/jquery/3.4.1/jquery.min.js",
      "https://js.kintone.com/jstree/3.3.8/jstree.min.js",
      "js/desktop.js"
    ],
    "css": [
      "css/51-modern-default.css",
      "css/desktop.css",
      "https://js.kintone.com/jstree/3.3.8/themes/default/style.min.css"
    ]
  },
  "icon": "image/icon.png",
  "config": {
    "html": "html/config.html",
    "js": [
      "js/kintone-config-helper.js",
      "https://js.cybozu.com/jquery/3.3.1/jquery.min.js",
      "js/config.js"
    ],
    "css": [
      "css/51-modern-default.css",
      "css/config.css"
    ],
    "required_params": [
      "root",
      "dropdown",
      "text"
    ]
  },
  "name": {
    "en": "JsTree"
  },
  "description": {
    "en": "Organize text field into JsTree folders using a dropdown menu to choose the folder"
  },
  "mobile": {
    "js": [
      "https://js.cybozu.com/jquery/3.3.1/jquery.min.js",
      "js/mobile.js"
    ],
    "css": [
      "css/mobile.css"
    ]
  }
}